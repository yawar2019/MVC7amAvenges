@model  List<MVC7amAvenges.Models.EmployeeModel>
@{
    ViewBag.Title = "sendinfo5";
}

<h2>sendinfo5</h2>

<table class="table table-bordered table-condensed">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>